{
  "name": "@seenn/cli",
  "version": "0.1.0",
  "description": "Seenn CLI - Manage your Seenn account from the command line",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "seenn": "./bin/seenn.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "seenn",
    "cli",
    "push-notifications",
    "live-activity"
  ],
  "author": "Seenn <hello@seenn.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seenn-io/cli.git"
  },
  "homepage": "https://docs.seenn.io",
  "bugs": {
    "url": "https://github.com/seenn-io/cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "open": "^10.1.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
